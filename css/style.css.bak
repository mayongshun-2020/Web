    .copy .button{
        margin-left:60%;
        width:300px;
    }
    .copy .button ul li a{
        padding:0px 2px;
    }
    .copy p{
        line-height:30px;
        margin-left:60%;
    }
    .footer .seo{
        margin-top:20px;
    }
}